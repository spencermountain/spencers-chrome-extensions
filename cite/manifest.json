{
    "name": "Cite page in wikipedia",
    "description": "automatically create a wikipedia reference from this page",
    "version": "1.0",
    "background_page": "background.html",
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ],
    "browser_action": {
        "default_title": "Cite",
        "default_icon": "icon.png",
        "popup": "popup.html"
    },
    "omnibox": { "keyword" : "cite" }
}
